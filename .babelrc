{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "jsx-control-statements",
    "@babel/plugin-transform-react-inline-elements",
    "emotion"
  ]
}
